{
  "name": "ragishark-viewer",
  "description": "tshark monitor",
  "version": "0.1.0",
  "author": "ragingo",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack serve --config conf/webpack.dev.js",
    "start:prod": "webpack serve --compress --config conf/webpack.prod.js",
    "build": "yarn build:prod",
    "build:dev": "webpack --progress --config conf/webpack.dev.js",
    "build:prod": "webpack --progress --config conf/webpack.prod.js",
    "prebuild:dev": "yarn run clean && yarn run sass:dev",
    "prebuild:prod": "yarn run clean && yarn run sass:prod",
    "sass:dev": "sass --style expanded ./src/index.scss ./dist/index.css",
    "sass:prod": "sass --style compressed ./src/index.scss ./dist/index.css",
    "lint": "tslint -c conf/tslint.jsonc --fix --force './src/**/*.{ts,tsx}'",
    "clean": "rimraf dist && mkdir dist && touch dist/.gitkeep"
  },
  "sideEffects": [
    "*.css"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "html-loader": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
