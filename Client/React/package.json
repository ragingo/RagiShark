{
  "name": "ragishark-viewer",
  "description": "tshark monitor",
  "version": "0.1.0",
  "author": "ragingo",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack serve --config conf/webpack.dev.js",
    "start:prod": "webpack serve --compress --config conf/webpack.prod.js",
    "build": "yarn build:prod",
    "build:dev": "webpack --progress --config conf/webpack.dev.js",
    "build:prod": "webpack --progress --config conf/webpack.prod.js",
    "prebuild:dev": "yarn run clean && yarn run sass:dev",
    "prebuild:prod": "yarn run clean && yarn run sass:prod",
    "sass:dev": "sass --style expanded ./src/index.scss ./dist/index.css",
    "sass:prod": "sass --style compressed ./src/index.scss ./dist/index.css",
    "lint": "tslint -c conf/tslint.jsonc --fix --force './src/**/*.{ts,tsx}'",
    "clean": "rimraf dist && mkdir dist && touch dist/.gitkeep"
  },
  "sideEffects": [
    "*.css"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "babel-loader": "^8.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "html-loader": "^1.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
